<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    <!-- Suppress false positives for test dependencies -->
    <suppress>
        <notes><![CDATA[
        This suppresses false positives in test dependencies that don't affect production code.
        ]]></notes>
        <gav regex="true">.*:.*:.*test.*</gav>
        <cpe>cpe:/a:test:test</cpe>
    </suppress>
</suppressions>